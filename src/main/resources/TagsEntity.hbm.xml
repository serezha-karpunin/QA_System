<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="db.entities.TagsEntity" table="tags" schema="epamproject">
        <id name="idTags">
            <column name="id_tags" sql-type="int(11)"/>
            <generator class="identity"/>
        </id>
        <property name="idQuestion">
            <column name="id_question" sql-type="int(11)"/>
        </property>
        <property name="tag">
            <column name="tag" sql-type="varchar(45)" length="45"/>
        </property>
    </class>
</hibernate-mapping>